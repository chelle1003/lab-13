def bilangan_prima(n):
    number=n*100
    num=[]
    res=[]
    for m in range(1,number):
        for i in range(2,m):
            if(m%i==0):
                break
        else:
            num.append(m)
    for c in range(1,hasil+1):
        res.append(num[c])
    f=sum(res)
    print(f)
hasil=17
